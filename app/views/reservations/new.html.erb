<h1>Reservations#new</h1>
<p>Find me in app/views/reservations/new.html.erb</p>

<%= bootstrap_form_for @reservation , method: :post, url: reservations_confirm_path do |f|%>
  <p>
      <%= f.label :persons,"" %>
      <%= f.number_field :persons %>
    </p>
  <p>
    <%= f.label :total_price,"" %>
    <%= f.number_field :total_price %>
  </p>
  <p>
    <%= f.label :room_id,"" %>
    <%= f.number_field :room_id %>
  </p>
  <%= f.submit %>
<% end %>